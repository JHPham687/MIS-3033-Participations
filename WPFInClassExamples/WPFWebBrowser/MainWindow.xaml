        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPFWebBrowser"
        mc:Ignorable="d"
        Title="Let me Google that for you" Height="157" Width="800">
    <Grid Height="295" VerticalAlignment="Top">
        <Button x:Name="btnGo" Content="GO" HorizontalAlignment="Left" Margin="516,47,0,0" VerticalAlignment="Top" Click="Button_Click"/>
        <TextBox x:Name="txtQuery" HorizontalAlignment="Left" Margin="253,47,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="232"/>

    </Grid>
</Window>
