            <ColumnDefinition Width="121*"/>
            <ColumnDefinition Width="79*"/>
        </Grid.ColumnDefinitions>
        <Label x:Name="lblDiploma" Content="Diploma Information" HorizontalAlignment="Left" Margin="76,31,0,0" VerticalAlignment="Top" Height="33" Width="120"/>
        <Label x:Name="lblFirstName" Content="First Name&#xD;&#xA;" HorizontalAlignment="Left" Margin="30,67,0,0" VerticalAlignment="Top" Height="26"/>
